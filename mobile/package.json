{
  "name": "IntelligentThinkingMobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace IntelligentThinkingMobile.xcworkspace -scheme IntelligentThinkingMobile -configuration Release -destination generic/platform=iOS archive",
    "dev:android": "adb reverse tcp:8080 tcp:8080 && react-native run-android",
    "dev:ios": "react-native run-ios",
    "clean": "react-native clean-project-auto",
    "pods": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.74.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "^3.10.1",
    "react-native-svg": "^15.2.0",
    "react-native-webview": "^13.8.6",
    "react-native-device-info": "^11.1.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "react-native-keychain": "^8.2.0",
    "@react-native-community/netinfo": "^11.3.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-animatable": "^1.4.0",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-config": "^1.5.1",
    "react-native-flipper": "^0.212.0",
    "react-native-dotenv": "^3.4.9",
    "react-redux": "^9.1.2",
    "@reduxjs/toolkit": "^2.2.5",
    "redux-persist": "^6.0.0",
    "axios": "^1.7.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-elements": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native/babel-preset": "^0.74.83",
    "@react-native/eslint-config": "^0.74.83",
    "@react-native/metro-config": "^0.74.83",
    "@react-native/typescript-config": "^0.74.83",
    "@types/react": "^18.3.1",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
} 